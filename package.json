{
  "name": "blog-app",
  "version": "1.0.0",
  "private": true,
  "description": "blog-app",
  "author": "Pavel Vondra",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "refresh": "curl -X POST http://localhost:8000/__refresh",
    "typecheck": "tsc --noEmit",
    "storybook": "NODE_ENV=test start-storybook -p 6006",
    "test-storybook": "NODE_ENV=test test-storybook --watchAll",
    "build-storybook": "build-storybook",
    "test": "TZ=UTC jest --watchAll --coverage --verbose=true",
    "test:staged": "CI=true TZ=UTC jest --coverage",
    "cy:open": "cypress open gatsby develop",
    "cy:e2e": "develop http://localhost:8000 cy:open",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "lint:css": "stylelint './src/**/*.styled.tsx'",
    "lint:fix-css": "stylelint './src/**/*.styled.tsx' --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.1",
    "@fontsource/montserrat": "^4.5.12",
    "@react-icons/all-files": "^4.1.0",
    "@reduxjs/toolkit": "^1.9.0",
    "axios": "^1.0.0",
    "babel-plugin-styled-components": "^2.0.7",
    "buffer": "^6.0.3",
    "core-js": "^3.0.0",
    "easymde": "^2.18.0",
    "engine.io": ">=6.4.2",
    "esm": "^3.2.25",
    "gatsby": "^5.11.0",
    "gatsby-plugin-image": "^3.9.0",
    "gatsby-plugin-sharp": "^5.9.0",
    "gatsby-plugin-sitemap": "^6.9.0",
    "gatsby-plugin-styled-components": "^6.9.0",
    "gatsby-source-apiserver": "^2.1.8",
    "gatsby-source-filesystem": "^5.9.0",
    "gatsby-transformer-remark": "^6.9.0",
    "gatsby-transformer-remark-rehype": "^4.1.0-alpha.1",
    "gatsby-transformer-sharp": "^5.9.0",
    "got": ">=11.8.5",
    "node-fetch": ">=2.6.1",
    "openapi-typescript": "^6.2.4",
    "postcss": "^8.4.20",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-loader-spinner": "^5.3.4",
    "react-markdown": "^8.0.3",
    "react-redux": "^8.0.4",
    "react-scripts": "^5.0.1",
    "react-simplemde-editor": "^5.2.0",
    "redux-persist": "^6.0.0",
    "remark-gfm": "^3.0.1",
    "styled-components": "^6.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.6.3",
    "@storybook/addon-actions": "^6.5.0",
    "@storybook/addon-essentials": "^6.5.0",
    "@storybook/addon-interactions": "^6.5.0",
    "@storybook/addon-links": "^6.5.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/cli": "6.5.0",
    "@storybook/jest": "^0.2.1",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.0",
    "@storybook/preset-create-react-app": "^7.3.2",
    "@storybook/react": "6.5.0",
    "@storybook/react-docgen-typescript-plugin": "^1.0.1",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/node": "^20.1.3",
    "@types/randomatic": "^3.1.3",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.1.2",
    "babel-loader": "^9.1.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-preset-gatsby": "^3.9.0",
    "cypress": "^12.12.0",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "gatsby-plugin-tsconfig-paths": "^1.0.6",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.3",
    "jest-environment-jsdom": "^29.1.2",
    "jest-image-snapshot": "^6.2.0",
    "lint-staged": "^14.0.1",
    "msw": "^0.47.3",
    "msw-storybook-addon": "^1.6.3",
    "postcss-styled-syntax": "^0.4.0",
    "preact": ">=10",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "react-docgen-typescript-plugin": "^1.0.5",
    "react-test-renderer": "^18.2.0",
    "storybook": "^6.5.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-files": "^1.1.4",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.0.4",
    "yarn-audit-fix": "^9.3.10"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "msw": {
    "workerDirectory": "static"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run test:staged"
    ],
    "src/**/*.styled.{js,jsx,ts,tsx}": [
      "npm run lint:fix-css"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "npm run format"
    ]
  }
}
